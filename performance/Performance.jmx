          <stringProp name="WebDriverSampler.script">import org.apache.jmeter.samplers.SampleResult
import org.openqa.selenium.By
import org.openqa.selenium.Keys
import org.openqa.selenium.WebDriver
import org.openqa.selenium.support.ui.ExpectedConditions
import org.openqa.selenium.support.ui.WebDriverWait

String FAIL_BACK = &quot;You can ask me about:&quot;

WebDriver getDriver() {
    return WDS.browser
}

SampleResult getSampleResult() {
    return WDS.sampleResult
}

def info(String message) {
    WDS.log.info(message)
}

def fail(String message) {
    def sampleResult = getSampleResult()
    sampleResult.setSuccessful(false)
    sampleResult.setResponseMessage(message)
    throw new RuntimeException(message)
}

def waitForCssSelector(String cssSelector) {
    new WebDriverWait(getDriver(), 20, 50).
            until(ExpectedConditions.visibilityOfElementLocated(By.cssSelector(cssSelector)))
}

def find(String cssSelector) {
    WebDriver driver = getDriver()
    waitForCssSelector(cssSelector)
    def element = driver.findElement(By.cssSelector(cssSelector))
    if (!element) {
        fail(&quot;element not found by css selector $cssSelector&quot;)
    }
    return element
}

def click(String cssSelector) {
    find(cssSelector).click()
}

def sendMessage(String text) {
    def messageField = find(&quot;.new-message&quot;)
    messageField.sendKeys(text)
    messageField.sendKeys(Keys.ENTER)
}

def clickFirstButton() {
    click(&quot;.reply:nth-child(1)&quot;)
}

String getResponseText(int number, boolean addGroup = true) {
    def cssSelector = &quot;.${addGroup ? &quot;group-&quot; : &apos;&apos;}message:nth-child($number) p&quot;
    return find(cssSelector).text
}

def validateResponse(int number, boolean addGroup = true, String text) {
    def responseText = getResponseText(number, addGroup)
    if (!responseText.contains(text)) {
        fail(&quot;element found by selector but not contains text $text ($responseText)&quot;)
    }
}

def sampleResult = getSampleResult()

sampleResult.sampleStart()
getDriver().get(&quot;http://0.0.0.0/&quot;)
sampleResult.connectEnd()

click(&quot;.open-launcher&quot;)
sendMessage(&quot;Hi&quot;)
[
        &quot;Sara&quot;,
        &quot;By chatting to me you agree to our privacy policy.&quot;,
        &quot;I can help you get started&quot;
].eachWithIndex { String message, int i -&gt;
    validateResponse(i + 1, false, message)
}
sendMessage(&quot;What is Rasa X?&quot;)
validateResponse(4, &quot;Sorry, I&apos;m not sure I&apos;ve understood you correctly&quot;)
clickFirstButton()
validateResponse(6, &quot;Rasa X is a toolset that helps you leverage conversations to improve your assistant. Learn more about Rasa X here or read the blog post about why we developed it here.&quot;)
sendMessage(&quot;who developed and created you?&quot;)
validateResponse(8, &quot;engineers at Rasa&quot;)
sendMessage(&quot;which programming languages do you support?&quot;)
validateResponse(10, &quot;Rasa Open Source is written in Python&quot;)
sendMessage(&quot;When I use Rasa, Can I make bot speaking Hindi?&quot;)
validateResponse(12, &quot;Sorry, I&apos;m not sure I&apos;ve understood you correctly&quot;)
sendMessage(&quot;How about English?&quot;)
validateResponse(14, &quot;Hmm, not sure what you mean...&quot;)
sendMessage(&quot;Can you help me?&quot;)
validateResponse(16, FAIL_BACK)
sendMessage(&quot;admin@gmail.com&quot;)
validateResponse(18, FAIL_BACK)
sendMessage(&quot;Bye&quot;)

sampleResult.sampleEnd()
sampleResult.successful = true
</stringProp>
